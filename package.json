{
  "name": "bz-career",
  "version": "0.0.1",
  "description": "BZ Career",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "nodemon src/index.ts --exec ts-node",
    "lint": "eslint . --ext .ts",
    "dev:migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/core/config/database.config.ts",
    "sync-schema": "npx typeorm-ts-node-commonjs schema:sync -d ./src/core/config/database.config.ts",
    "enttity": "npx typeorm-ts-node-commonjs entity:run -d ./src/core/config/database.config.ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "migration:show": "npm typeorm migration:show",
    "migration:create": "node ./src/core/config/database.config.ts",
    "migration:revert": "npm typeorm migration:revert"
  },
  "dependencies": {
    "@bz-analytics/logger": "^0.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.0",
    "fs": "0.0.1-security",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-saml": "^3.2.4",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^13.13.52",
    "@types/passport": "^1.0.16",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "commitlint-plugin-function-rules": "^1.7.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^6.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
